# -*-shell-script-*-
# ~/.profile: executed by Bourne-compatible login shells.

# Function to help us manage paths
. $HOME/.bashrc.d/sys-functions.sh

add_to_path /usr/local/scripts
add_to_path ~/scripts
add_to_path ~/bin
add_to_path ~/.local/bin
add_to_path ~/django_sites/scripts last

MAILCHECK=60

